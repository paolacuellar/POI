<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <item android:top="28dp"
        android:width="20dp"
        android:height="20dp"
        android:gravity="left">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="0%"
            android:pivotY="0%"
            android:toDegrees="0" >
            <shape android:shape="rectangle" >
                <size android:height="30dp"/>
                <solid android:color="@color/white" />
                <corners
                    android:radius="3dp"
                    />
                <stroke
                    android:color="@color/coffee"
                    android:width="2dp"
                    />
            </shape>
        </rotate>
    </item>

    <item
        android:left="12dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners
                android:radius="14dp"
                />
            <stroke
                android:color="@color/coffee"
                android:width="2dp"
                />
            <solid
                android:color="@color/white"/>
        </shape>
    </item>

    <item android:top="14dp"
        android:left="16dp"
        android:width="20dp"
        android:height="20dp"
        android:gravity="left">
        <rotate
            android:fromDegrees="45"
            android:pivotX="0%"
            android:pivotY="0%"
            android:toDegrees="0" >
            <shape android:shape="rectangle" >
                <size android:height="20dp"/>
                <solid android:color="@color/white" />
                <corners
                    android:radius="3dp"
                    />
            </shape>
        </rotate>
    </item>

</layer-list>


